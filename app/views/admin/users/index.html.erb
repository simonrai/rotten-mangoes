<h1>Rotten Mangoes - All Users</h1>

<%= link_to "Back to all movies", movies_path %><br/>

<h3>List of Users</h3>

<table class="table">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Created</th>
    <th>Details</th>
  </tr>
  <% @user.each do |user| %>
  <tr>
    <td><%= link_to user.full_name, admin_user_path(user) %></td>
    <td><%= user.email %></td>
    <td><%= user.created_at %></td>
    <td><%= user.email %></td>
  </tr>  
  <% end %>
</table>